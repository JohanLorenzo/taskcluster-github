{
  "name": "taskcluster-github",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "compile": "babel-compile -p taskcluster src:lib test:.test",
    "lint": "eslint src/*.js test/*.js",
    "test": "mocha .test/*_test.js",
    "pretest": "yarn lint && yarn compile",
    "install": "yarn compile",
    "checkStaging": "yarn compile && node .test/checkStaging.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taskcluster/taskcluster-github.git"
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "azure-entities": "^1.0.3",
    "babel-compile": "^2.0.0",
    "babel-preset-taskcluster": "^3.0.0",
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.4.7",
    "debug": "^2.1.3",
    "eslint-config-taskcluster": "^3.0.0",
    "github": "8.2.1",
    "js-yaml": "^3.8.1",
    "json-parameterization": "^0.2.0",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.11.1",
    "mocha": "^2.2.1",
    "pulse-publisher": "^1.2.0",
    "sinon": "^1.17.6",
    "slugid": "^1.1.0",
    "source-map-support": "^0.4.0",
    "taskcluster-client": "^2.3.0",
    "taskcluster-lib-api": "3.2.2",
    "taskcluster-lib-app": "^1.0.0",
    "taskcluster-lib-docs": "^3.3.0",
    "taskcluster-lib-loader": "^1.0.0",
    "taskcluster-lib-monitor": "^4.3.1",
    "taskcluster-lib-testing": "^1.0.3",
    "taskcluster-lib-validate": "^2.0.0",
    "typed-env-config": "^1.1.0"
  },
  "engine-strict": true,
  "engines": {
    "node": "^8.0.0",
    "yarn": "^1.0.0"
  },
  "devDependencies": {
    "fs-extra": "^2.0.0",
    "got": "^6.7.1"
  }
}
